<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/background_disable_button_solid"
                />
            <stroke
                android:width="@dimen/stroke_width"
                android:color="@color/grey"
                />
            <corners android:radius="@dimen/frame_radius"
                />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape>
            <solid android:color="@color/background_enable_button_solid"
                />
            <stroke
                android:width="@dimen/stroke_width"
                android:color="@color/background_enable_button_solid"
                />
            <corners android:radius="@dimen/frame_radius"
                />
        </shape>
    </item>
</selector>